{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2021",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "noEmit": false,
    "declaration": false,
    "skipLibCheck": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
